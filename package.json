    "clean": "rm -rf ./dist",
    "create:component": "node scripts/scaffold-component.js",
    "format": "prettier --write .",
    "lint": "eslint './**/*.{ts,tsx}' --max-warnings 0",
    "lint:ts": "tsc",
    "lint:css": "stylelint **/*.{scss,css} --custom-syntax postcss",
    "lint:css:fix": "stylelint --fix **/*.{scss,css} --custom-syntax postcss",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "rollup": "yarn clean && rollup -c"
