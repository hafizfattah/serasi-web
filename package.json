  "dependencies": {
    "classnames": "^2.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-feather": "^2.0.10"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
