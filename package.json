    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "storybook-css-modules": "^1.0.8",
    "style-loader": "^3.3.1",
    "stylelint": "^14.9.1",
    "stylelint-checkstyle-formatter": "^0.1.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-scss": "^4.2.0",
    "tailwindcss": "^3.1.5",
    "typescript": "4.6.3"
