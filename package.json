{
  "name": "@hafizfattah/serasi-web",
  "author": "hafiz fattah",
  "description": "Another react component library",
  "version": "0.1.1",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0",
    "yarn": "^1.22.0",
    "npm": "please use yarn"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "prepare": "husky install",
    "clean": "rm -rf ./dist",
    "create:component": "node scripts/scaffold-component.js",
    "test": "jest",
    "test:generate-output": "jest --json --outputFile=./coverage/jest-test-results.json",
    "format": "prettier --write .",
    "lint": "eslint './**/*.{ts,tsx}' --max-warnings 0",
    "lint:ts": "tsc",
    "lint:css": "stylelint **/*.{scss,css} --custom-syntax postcss-scss",
    "lint:css:fix": "stylelint **/*.{scss,css} --fix --custom-syntax postcss-scss",
    "storybook": "start-storybook -p 6006",
    "build": "yarn clean && yarn build:bundle && yarn build:post",
    "build:bundle": "rollup -c",
    "build:pre": "node scripts/build-entry-component.js",
    "build:post": "rm -rf dist/cjs/components && rm -rf dist/esm/components",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@storybook/addon-a11y": "^6.5.9",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-jest": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-viewport": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.9",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.6",
    "@types/node": "17.0.23",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^28.1.3",
    "babel-loader": "^8.2.5",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "cssnano": "^5.1.12",
    "eslint": "8.18.0",
    "eslint-plugin-storybook": "^0.6.0",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^8.1.2",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.76.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "storybook-addon-performance": "^0.16.1",
    "storybook-addon-themes": "^6.1.0",
    "storybook-css-modules": "^1.0.8",
    "style-loader": "^3.3.1",
    "stylelint": "^14.9.1",
    "stylelint-checkstyle-formatter": "^0.1.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-scss": "^4.2.0",
    "tailwindcss": "^3.1.5",
    "typescript": "4.6.3"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "dependencies": {
    "react-icons": "^4.4.0"
  }
}
